{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Scrape Extensions",
      "program": "${workspaceFolder}/scripts/scrapeBuiltinExtensions.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "args": ["--runInBand"],
      "console": "integratedTerminal",
      "disableOptimisticBPs": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example Site",
      "cwd": "${workspaceFolder}/examples/example-site",
      "program": "${workspaceFolder}/examples/example-site/node_modules/.bin/gatsby",
      "args": ["develop"],
      "console": "integratedTerminal",
      "disableOptimisticBPs": true
    }
  ]
}